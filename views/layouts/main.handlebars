<!-- ! to gen the template -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- go to https://getbootstrap.com/ and search for Include via CDN -->
    <!-- import Boostrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">


    <!-- change title to Main Page -->
    <title>Main Page</title>
    
    <!-- add the favicon with the following HTML code -->
    <!-- the favicon displayed to the left of the page title in the browser tab -->
    <link rel="icon" type="image/x-icon" href="/img/favicon.ico">

</head>
<body>
    
    
    {{> _navbar}}
    
    <!-- put a container for body, format the content better -->
    <div class="container">
        
        <!-- add error message part before body (used when render page) -->
        {{> _error}}

        <!-- add msg part before body (used by req.flash()) -->
        {{> _msg}} 
        
        <!-- add the following line, must be 3 brackets -->
        {{{body}}}
    </div>

    

    <!-- import Boostrap JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
</body>
</html>